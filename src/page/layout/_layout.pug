doctype html
html
	head
		block title
			title layout
			meta(charset="UTF-8")
			meta(http-equiv="X-UA-Compatible" content="IE=edge")
			meta(name="viewport" content="width=device-width, initial-scale=1.0")
			link(type="text/css" href="app.css?v=##version##" rel="stylesheet")
	block changeBgColor
	body(class=`${bodyClass}`)
		.wrapper
			block header
			block content
			block footer
		block login
	block scripts
		
- var bodyClass
		